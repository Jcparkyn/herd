{
  x = [:];
  y = x;
  set y.a = 'a';
  print x;
  print y;
}

{
  order1 = [ status: [ dispatched: false ]];
  order2 = order1;
  set order2.status.dispatched = true;

  dispatch = { order ->
    set order.status.dispatched = true;
    set order.status.dispatched_at = '2024-07-20';
    order
  };
  order3 = dispatch order1;

  print order1;
  print order2;
  print order3;
}