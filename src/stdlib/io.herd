
print = \val\ BUILTIN:print val;

println = \val\ (
  BUILTIN:print val;
  BUILTIN:print '\n';
);

// This will be replaced by println once variadic functions are implemented.
printf = \arr\ (
  for x in arr do (
    BUILTIN:print x;
  )
  BUILTIN:print '\n';
);

readline = \\ (
  BUILTIN:readline
);

return {
  print,
  println,
  printf,
  readline,
};
